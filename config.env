# 小米10 Pro(cmi)专属设备代号
DEVICE=cmi
# 小米官方MIUI14安卓13内核源码（cmi-t-oss分支正确）
KERNEL_SOURCE=https://github.com/MiCode/Xiaomi_Kernel_OpenSource.git
KERNEL_BRANCH=cmi-t-oss
# 【核心修正】官方正确的defconfig路径，解决找不到配置的报错
KERNEL_CONFIG=vendor/cmi_defconfig
KERNEL_IMAGE_NAME=Image.gz-dtb
ARCH=arm64
ADD_LOCALVERSION_TO_FILENAME=false
EXTRA_CMDS=LD=ld.lld

# Clang编译器配置（适配cmi-t-oss的稳定版本）
USE_CUSTOM_CLANG=false
CLANG_VERSION=r416183b

# SukiSU核心配置（正确无误）
KSU_REPO=https://github.com/SukiSU-Ultra/SukiSU-Ultra
KSU_BRANCH=nongki

# 开启必要配置，保证编译成功+功能正常
ENABLE_LTO=true
ENABLE_KPROBES=true
NEED_OVERLAYFS_CONFIG=true
